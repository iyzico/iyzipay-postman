									"var initializeBkm = {",
									"    locale: null,",
									"    conversationId: null,",
									"    price: null,",
									"    basketId: null,",
									"    paymentGroup: null,",
									"    buyer: buyer,",
									"    shippingAddress: shippingAddress,",
									"    billingAddress: billingAddress,",
									"    basketItems: basketItems,",
									"    callbackUrl:null,",

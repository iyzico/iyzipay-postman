									"    for (var member in obj) {",
									"        ",
									"        if(obj[member] === null) {    ",
									"            delete obj[member];",
									"        }",
									"        else if (typeof obj[member] === 'object'){",
									"            obj[member]=nullClear(obj[member]);",
									"            if(Object.keys(obj[member]).length===0){",
									"                delete obj[member];",
									"            }",
									"        }",
									"    }",
									"    ",
									"    return obj;",
									"}",

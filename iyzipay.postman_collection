									"};",
									"",
									"function BasketItem() {",
									"    this.id= null;",
									"    this.price= null;",
									"    this.name=null;",
									"    this.category1= null;",
									"    this.category2= null;",
									"    this.itemType=null,",
									"    this.subMerchantKey=null;",
									"    this.subMerchantPrice= null;",
									"}",

									"var basketItems = [];",
									"var enabledInstallments = [];",
									"",
									"var payment = {",
									"    locale: null,",
									"    conversationId: null,",
									"    price: null,",
									"    paidPrice: null,",
									"    installment: null,",
									"    paymentChannel: null,",
									"    basketId: null,",
									"    paymentGroup: null,",
									"    paymentCard: paymentCard,",
									"    buyer: buyer,",
									"    shippingAddress: shippingAddress,",
									"    billingAddress: billingAddress,",
									"    basketItems: basketItems,",
									"    currency: null,",
									"    ",
									"};",
